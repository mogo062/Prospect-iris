<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/util 
		http://www.springframework.org/schema/util/spring-util.xsd
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context.xsd"
		default-lazy-init="true">

<!-- Define Spring bean for resource : ODataMetadata -->
<!-- begin spring bean for state : Prospect_Prospect-ODataMetadata -->
<bean id="Prospect_Prospect-ODataMetadata" class="com.temenos.interaction.core.hypermedia.ResourceState">
	<constructor-arg name="entityName" value="Metadata" />
	<constructor-arg name="name" value="ODataMetadata" />
	<constructor-arg>
		<list>
		<bean class="com.temenos.interaction.core.hypermedia.Action">
			<constructor-arg value="GETMetadata" />
			<constructor-arg value="VIEW" />
		</bean>
		</list>
	</constructor-arg>
	<constructor-arg name="path" value="/{companyid}/$metadata" />
	<constructor-arg name="rels">
		<null />
	</constructor-arg>
	<constructor-arg name="uriSpec"><bean class="com.temenos.interaction.core.hypermedia.UriSpecification"><constructor-arg name="name" value="ODataMetadata" /><constructor-arg name="template" value="/{companyid}/$metadata" /></bean></constructor-arg>
	<constructor-arg name="errorState"><null /></constructor-arg>
	
	<!-- Start property transitions list -->
	<property name="transitions">
		<list>
		<!-- create transitions  -->
		</list>
	</property>
</bean>	
<!-- end spring bean for state : Prospect_Prospect-ODataMetadata -->		
<!-- Define Spring bean for resource : ServiceDocument -->
<!-- begin spring bean for state : Prospect_Prospect-ServiceDocument -->
<bean id="Prospect_Prospect-ServiceDocument" class="com.temenos.interaction.core.hypermedia.ResourceState">
	<constructor-arg name="entityName" value="ServiceDocument" />
	<constructor-arg name="name" value="ServiceDocument" />
	<constructor-arg>
		<list>
		<bean class="com.temenos.interaction.core.hypermedia.Action">
			<constructor-arg value="GETServiceDocument" />
			<constructor-arg value="VIEW" />
		</bean>
		</list>
	</constructor-arg>
	<constructor-arg name="path" value="/{companyid}/" />
	<constructor-arg name="rels">
		<null />
	</constructor-arg>
	<constructor-arg name="uriSpec"><bean class="com.temenos.interaction.core.hypermedia.UriSpecification"><constructor-arg name="name" value="ServiceDocument" /><constructor-arg name="template" value="/{companyid}/" /></bean></constructor-arg>
	<constructor-arg name="errorState"><null /></constructor-arg>
	
	<property name="initial" value="true" />
	<!-- Start property transitions list -->
	<property name="transitions">
		<list>
		<!-- create transitions  -->
	<!-- begin transition : Prospect_Prospect-T24FieldMetadata -->
	<bean class="com.temenos.interaction.springdsl.TransitionFactoryBean">
		<property name="method" value="GET" />
		<property name="target"><bean class="com.temenos.interaction.core.hypermedia.LazyCollectionResourceState"><constructor-arg name="name" value="Prospect_Prospect-T24FieldMetadata" /></bean>
		
		</property>
<property name="uriParameters"><util:map></util:map></property>
<property name="evaluation"><null /></property>
		<property name="label" value="T24FieldMetadata" />
	</bean>
	<!-- end transition : Prospect_Prospect-T24FieldMetadata -->
	<!-- begin transition : T24_enqCustomer-enqCustomer -->
	<bean class="com.temenos.interaction.springdsl.TransitionFactoryBean">
		<property name="method" value="GET" />
		<property name="target"><bean class="com.temenos.interaction.core.hypermedia.LazyResourceState"><constructor-arg name="name" value="T24_enqCustomer-enqCustomer" /></bean>
		
		</property>
<property name="uriParameters"><util:map></util:map></property>
<property name="evaluation"><null /></property>
		<property name="label" value="enqCustomer" />
	</bean>
	<!-- end transition : T24_enqCustomer-enqCustomer -->
	<!-- begin transition : T24_enqAccountList-enqAccountList -->
	<bean class="com.temenos.interaction.springdsl.TransitionFactoryBean">
		<property name="method" value="GET" />
		<property name="target"><bean class="com.temenos.interaction.core.hypermedia.LazyResourceState"><constructor-arg name="name" value="T24_enqAccountList-enqAccountList" /></bean>
		
		</property>
<property name="uriParameters"><util:map></util:map></property>
<property name="evaluation"><null /></property>
		<property name="label" value="enqAccountList" />
	</bean>
	<!-- end transition : T24_enqAccountList-enqAccountList -->
	<!-- begin transition : T24_enqAccountList-enqAccountListByCustomer -->
	<bean class="com.temenos.interaction.springdsl.TransitionFactoryBean">
		<property name="method" value="GET" />
		<property name="target"><bean class="com.temenos.interaction.core.hypermedia.LazyCollectionResourceState"><constructor-arg name="name" value="T24_enqAccountList-enqAccountListByCustomer" /></bean>
		
		</property>
<property name="uriParameters"><util:map></util:map></property>
<property name="evaluation"><null /></property>
		<property name="label" value="enqAccountListByCustomer" />
	</bean>
	<!-- end transition : T24_enqAccountList-enqAccountListByCustomer -->
	<!-- begin transition : common_Errors-Errors -->
	<bean class="com.temenos.interaction.springdsl.TransitionFactoryBean">
		<property name="method" value="GET" />
		<property name="target"><bean class="com.temenos.interaction.core.hypermedia.LazyCollectionResourceState"><constructor-arg name="name" value="common_Errors-Errors" /></bean>
		
		</property>
<property name="uriParameters"><util:map></util:map></property>
<property name="evaluation"><null /></property>
		<property name="label" value="Errors" />
	</bean>
	<!-- end transition : common_Errors-Errors -->
		</list>
	</property>
</bean>	
<!-- end spring bean for state : Prospect_Prospect-ServiceDocument -->		
<!-- Define Spring bean for resource : InteractionException -->
<!-- begin spring bean for state : Prospect_Prospect-InteractionException -->
<bean id="Prospect_Prospect-InteractionException" class="com.temenos.interaction.core.hypermedia.ResourceState">
	<constructor-arg name="entityName" value="InteractionException" />
	<constructor-arg name="name" value="InteractionException" />
	<constructor-arg>
		<list>
		<bean class="com.temenos.interaction.core.hypermedia.Action">
			<constructor-arg value="GETException" />
			<constructor-arg value="VIEW" />
		</bean>
		</list>
	</constructor-arg>
	<constructor-arg name="path" value="/{companyid}/InteractionException" />
	<constructor-arg name="rels">
		<null />
	</constructor-arg>
	<constructor-arg name="uriSpec"><null /></constructor-arg>
	<constructor-arg name="errorState"><null /></constructor-arg>
	
	<property name="exception" value="true" />
	<!-- Start property transitions list -->
	<property name="transitions">
		<list>
		<!-- create transitions  -->
		</list>
	</property>
</bean>	
<!-- end spring bean for state : Prospect_Prospect-InteractionException -->		
<!-- Define Spring bean for resource : T24FieldMetadata -->
<!-- begin spring bean for state : Prospect_Prospect-T24FieldMetadata -->
<bean id="Prospect_Prospect-T24FieldMetadata" class="com.temenos.interaction.core.hypermedia.CollectionResourceState">
	<constructor-arg name="entityName" value="T24FieldMetadata" />
	<constructor-arg name="name" value="T24FieldMetadata" />
	<constructor-arg>
		<list>
		<bean class="com.temenos.interaction.core.hypermedia.Action">
			<constructor-arg value="T24FieldMetadata" />
			<constructor-arg value="ENTRY" />
		</bean>
		</list>
	</constructor-arg>
	<constructor-arg name="path" value="/{companyid}/T24FieldMetadata()" />
	<constructor-arg name="rels">
		<array>
		<value><![CDATA[http://temenostech.temenos.com/rels/metadata]]></value>
		</array>
	</constructor-arg>
	<constructor-arg name="uriSpec"><bean class="com.temenos.interaction.core.hypermedia.UriSpecification"><constructor-arg name="name" value="T24FieldMetadata" /><constructor-arg name="template" value="/{companyid}/T24FieldMetadata()" /></bean></constructor-arg>
	<constructor-arg name="errorState"><null /></constructor-arg>
	
	<!-- Start property transitions list -->
	<property name="transitions">
		<list>
		<!-- create transitions  -->
		</list>
	</property>
</bean>	
<!-- end spring bean for state : Prospect_Prospect-T24FieldMetadata -->		
</beans>
