domain common {
    use common.T24Commands.*
    use common.HTTPEvents.*
    use common.CoreCommands.*
        
    rim Messages {  
        resource ProcessMessages {
            type: item
            entity: Messages
            commands [ GET : MessageCommand ]
            relations [ "http://temenostech.temenos.com/rels/messages"]
            path: "/messages"
        }
    }    
}
